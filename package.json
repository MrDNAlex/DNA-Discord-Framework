    "Library",
    "Module",
    "Package",
    "NPM",
    "Node",
    "Module",
    "Package",
    "Manager",
    "Discord.js",
    "DNA",
    "NanoDNA"
  ],
  "author": "Mr DNA",
  "license": "ISC",
  "dependencies": {
    "@types/readline-sync": "1.4.8",
    "axios": "^1.7.7",
    "discord.js": "^14.17.3",
    "follow-redirects": "^1.15.6",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "readline-sync": "^1.4.10",
    "ssh2": "^1.15.0",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nano-DNA-Studios/DNA-Discord-Framework.git"
  },
